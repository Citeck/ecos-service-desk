            "attrs": [
              {
                "attr": "",
                "value": ""
              }
            ],
            "content": {
              "ru": "<style>\n\n    .remaining_time_ext, .remaining_time_ext_soon_expired, .remaining_time_ext_expired {\n        position: relative;\n        height: 30px;\n        width: 100%;\n        margin-top: 20px;\n        border-radius: 40px;\n    }\n\n    .background_block {\n        position: absolute;\n        top: 0;\n        left: 0;\n        height: 100%;\n        width: 110px;\n        border-radius: 40px;\n        z-index: 0;\n    }\n\n    .remaining_time_ext_expired .background_block {\n        background-color: red;\n    }\n    \n    .remaining_time_ext_soon_expired .background_block {\n        background-color: #ff5442;\n    }\n\n    .remaining_time_int {\n        display: flex;\n        align-items: center;\n        line-height: 30px;\n        color: #b7b7b7;\n        position: relative;\n        z-index: 1;\n        margin-left: 10px;\n    }\n\n\n    .remaining_time_ext .icon-small-clock {\n        color: #b7b7b7;\n        margin-top: 3px;\n    }\n\n    .remaining_time_ext_soon_expired .icon-small-clock {\n        color: white;\n        margin-top: 3px;\n    }\n\n    .remaining_time_ext_soon_expired .remaining_time_int {\n        color: white;\n    }\n    \n    .remaining_time_ext_soon_expired .remaining_time_text_time {\n        color: white;\n    }\n\n    .remaining_time_ext_expired .icon-small-clock {\n        color: white;\n        margin-top: 3px;\n    }\n\n    .remaining_time_ext_expired .remaining_time_int {\n        color: white;\n    }\n    \n    .remaining_time_ext_expired .remaining_time_text_time {\n        color: white;\n    }\n\n    .remaining_time_text_time {\n        white-space: nowrap;\n        margin-right: 3px;\n    }\n    \n    #text_info {\n        color: #b7b7b7;\n        position: absolute;\n        margin-left: 105px;\n        width: calc(100% - 105px);\n        overflow: hidden;\n        white-space: nowrap;\n        text-overflow: ellipsis;\n    }\n\n</style>\n    \n\n<div>\n    <div class=\"background_block\"></div>\n    <p class=\"remaining_time_int\">\n        <strong class=\"remaining_time_text_time\">\n            {{data.remainingTimeFirstLine}}\n        </strong>\n        <b id=\"text_info\" title=\"1-я линии ТП\">| 1-я линии ТП</b>\n    </p>\n</div>\n"
            },
            "refreshOnChange": true,
            "refreshOn": [],
            "optionalWhenDisabled": false,
            "logic": [
              {
                "name": "Change background-color expired",
                "trigger": {
                  "type": "javascript",
                  "javascript": "var remainingTimeInMinutes = data.remainingTime.remainingTimeFirstLineSupport;\n\nif (remainingTimeInMinutes !== undefined &&\n    remainingTimeInMinutes !== null &&\n    remainingTimeInMinutes <= 0) {\n  result = true;\n} else {\n  result = false;\n}"
                },
                "actions": [
                  {
                    "name": "Change CSS-class expired",
